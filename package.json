{
  "name": "task-contact-managment",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"json-server --watch ./db.json\" \"vite\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "prettier": "prettier --write",
    "preview": "concurrently \"json-server --watch ./db.json\" \"vite preview\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "@tanstack/react-form": "^0.41.1",
    "@tanstack/react-query": "^5.64.1",
    "@tanstack/react-query-devtools": "^5.64.1",
    "@tanstack/react-router": "^1.95.5",
    "@tanstack/zod-form-adapter": "^0.41.1",
    "json-server": "^1.0.0-beta.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tanstack/router-devtools": "^1.95.5",
    "@tanstack/router-plugin": "^1.95.5",
    "@types/node": "^22.10.6",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "lucide-react": "^0.471.1",
    "postcss": "^8.5.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
